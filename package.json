{
  "name": "next-neodog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:dev": "env-cmd -f ./env/.env.local next build",
    "build:staging": "env-cmd -f ./env/.env.staging next build",
    "build:prod": "env-cmd -f ./env/.env.production next build",
    "start": "next start",
    "start:dev": "env-cmd -f ./env/.env.local next dev --turbopack --port 3001",
    "start:staging": "env-cmd -f ./env/.env.staging next dev --turbopack --port 3001",
    "start:prod": "env-cmd -f ./env/.env.production next dev --turbopack --port 3001",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "chartjs-plugin-annotation": "^3.1.0",
    "clsx": "^1.1.1",
    "date-fns": "^4.1.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^10.1.5",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-date-range": "^2.0.1",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "zod": "^3.25.51",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-date-range": "^1.4.10",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "prettier": "3.5.3",
    "sass": "^1.89.0",
    "typescript": "^5"
  }
